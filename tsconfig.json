{
  "compilerOptions": {
    "noEmit": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
    "target": "es2015",
    "moduleResolution": "node"
  },
  "exclude": ["install", "node_modules", "plugin-test", "plugins-add", "setup"]
}
